{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.34/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.json",
  "resize_delta": 50,
  "animation": {
    "enabled": true,
    "duration": 650,
    "fps": 240,
    "style": {
      "movement": "EaseOutBack",
      "transparency": "Linear"
    }
  },
  "border": false,
  "border_colours": {
    "unfocused": "#00000000",
    "stack": "#ffffff",
    "monocle": "#ffffff",
    "single": "#ffffff"
  },
  "border_implementation": "Komorebi",
  "border_offset": -1,
  "border_style": "Rounded",
  "border_width": 4,
  "cross_monitor_move_behaviour": "Insert",
  "default_container_padding": 10,
  "default_workspace_padding": 4,
  "float_override": false,
  "global_work_area_offset": {
    "left": 0,
    "top": 0,
    "bottom": 0,
    "right": 0
  },
  "mouse_follows_focus": true,
  "stackbar": {
    "tabs": {
      "font_size": 0,
      "background": "#ffffff",
      "font_family": "Segoe UI",
      "unfocused_text": "#ffffff",
      "width": 300,
      "focused_text": "#ffffff"
    },
    "height": 40,
    "label": "Process",
    "mode": "OnStack"
  },
  "transparency": false,
  "transparency_alpha": 230,
  "unmanaged_window_operation_behaviour": "Op",
  "window_container_behaviour": "Create",
  "window_hiding_behaviour": "Minimize",
  "monitors": [
    {
      "workspaces": [
        {
          "name": "一",
          "layout": "BSP"
        },
        {
          "name": "二",
          "layout": "BSP"
        },
        {
          "name": "三",
          "layout": "BSP"
        },
        {
          "name": "四",
          "layout": "BSP"
        },
        {
          "name": "五",
          "layout": "BSP"
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "一",
          "layout": "Rows"
        },
        {
          "name": "二",
          "layout": "Rows"
        },
        {
          "name": "三",
          "layout": "Rows"
        },
        {
          "name": "四",
          "layout": "Rows"
        },
        {
          "name": "五",
          "layout": "Rows"
        }
      ]
    }
  ]
}
